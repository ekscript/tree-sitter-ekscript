{
  "name": "tree-sitter-ekscript",
  "version": "0.2.4",
  "description": "EkScript grammar for tree-sitter",
  "main": "index.js",
  "scripts": {
    "gen": "tree-sitter generate",
    "test": "tree-sitter test",
    "dev": "nodemon --watch grammar.js --watch corpus/**/*.txt -e js,txt --exec \"npm run build && npm run test\"",
    "build": "node-gyp configure && node-gyp build",
    "publish": "np --no-tests patch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ekscript/tree-sitter-ekscript"
  },
  "keywords": [
    "ekscript",
    "tree-sitter",
    "parser",
    "grammar",
    "highlight"
  ],
  "author": "Himujjal",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.14.2",
    "tree-sitter": "^0.17.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "tree-sitter-cli": "^0.18.0"
  }
}
